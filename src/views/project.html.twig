      <section class='l-section l-section-narrow'></section>
      <section class='l-section l-section-reviews b-skew b-skew-bottom light'>
        <div class='bg-top'></div>
        <div class='l-section__body b-skew b-skew-top light'>
          <h2 class='s-title'>Отзывы</h2>
			{% for one_review in reviews %}
				<div class='b-bordered-block b-review'>
					<div class='b-review__top'>
					  <div class='avatar'><img width="128" height="128" src="/project_files/{{project.id}}/reviews/{{one_review.author_avatar}}" /></div>
					  <div class='name'>{{one_review.author_name|raw}}</div>
					  <div class='company'>{{one_review.author_company|raw}}</div>
					  <div class='position'>{{one_review.author_position|raw}}</div>
					</div>
					<div class='b-review__bottom'>
					  <p>{{one_review.review_text|raw}}</p>
					</div>
				</div>
			{% endfor %}
        </div>
      </section>
      <section class='l-section l-section-apply' id='apply'>
        <h2 class='s-title white'>Заявка</h2>
        <form class='s-form b-apply'>
          <div class='s-form-group'>
            <label for='name'>Контактное лицо</label>
            <input class='s-control' id='name' name='name' placeholder='Как к вам обращаться?' size='40' type='text'>
          </div>
          <div class='s-form-group'>
            <input class='s-control' id='email' name='email' placeholder='Ваш e-mail' size='20' type='email'>
          </div>
          <div class='s-form-group'>
            <input class='s-control' id='phone' name='phone' placeholder='Ваш телефон' size='20' type='text'>
          </div>
          <div class='s-form-group'>
            <label class='inline' for='date'>Когда</label>
            <input class='s-control date' id='date' name='date' size='12' type='text'>
            <label class='inline' for='qty'>Человек</label>
            <input type="number" name="qty" value="0" max="1000" style='width: 60px'>
          </div>
          <div class='s-form-group'>
            <textarea class='s-control textarea' id='descr' name='descr' placeholder='Подробности' rows='6'></textarea>
          </div>
          <button class='s-button block' type='submit'>Отправить</button>
        </form>
      </section>
