                    <li class='item active' data-action='{{one_project_type.name}}'>
                {% else %}
                    <li class='item' data-action='{{one_project_type.name}}'>
                {% endif %}
                    {{one_project_type.svg|raw}}
                    <p class='icontext'>{{one_project_type.full_name}}</p>
