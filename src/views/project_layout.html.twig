    <main class='l-content'>
    {% block content %}
    {% endblock %}
      <section class='l-section l-section-projects b-skew b-skew-top dark'>
        <h2 class='s-title main-color'>Другие проекты</h2>
        <div class='b-slider b-slider-main' id='slider'>
          <ul class='list'>
                {% for one_project in projects %}
                    <li class='item b-project' data-type='{% for one_data_type in one_project.data_type %}{% if loop.first %}{{one_data_type.name}}{% else %}, {{one_data_type.name}}{% endif %}{% endfor %}'>
                        <a href="/project/{{one_project.id}}">
                            <div class='b-project__image' style='background-image: url(/project_files/{{one_project.id}}/slider/big_slider_image.jpg)'></div>
                            <div class='b-project__image--small' style='background-image: url(/project_files/{{one_project.id}}/slider/small_slider_image.jpg)'></div>
                            <div class='b-project__body'>
                                <h3>{{one_project.name|raw}}</h3>
                                <p class="main">{{one_project.long_description|raw}}</p>
                                <p class="preview">{{one_project.short_description|raw}}</p>
                            </div>
                        </a>
                    </li>
                {% endfor %}
            </ul>
