        <nav class='l-header__nav'>
          <ul class='l-header__menu'>
            <li class='item'>
              <a href="/">Проекты</a>
              <ul class='l-header__submenu js-slider-control'>
                {% for one_project_type in project_types %}
                    <li class='item'><a href="/?{{one_project_type.name}}">{{one_project_type.full_name}}</a></li>
                {% endfor %}
              </ul>
            <li class='item'>
              <a href="/about">О компании</a>
              <ul class='l-header__submenu'>
                <li class='item'><a href="/about#team">Команда</a></li>
                <li class='item'><a href="/about#clients">Клиенты</a></li>
                <li class='item'><a href="/about#partners">Партнеры</a></li>
              </ul>
            </li>
          </ul>
        </nav>
        <div class='l-header__actions'>
          {% block apply %}
          {% endblock %}
          <div class='phone'>(812) 574 65 12</div>
          <div class='phone'>(921) 976 38 76</div>
